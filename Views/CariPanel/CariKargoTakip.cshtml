@using MvcWebTicariOtomasyon.Models.Siniflar;
@model List<KargoTakip>
@{
    ViewBag.Title = "CariKargoTakip";
    Layout = "~/Views/Shared/_CariLayout.cshtml";
}

<br />
<h2>Cari Kargo Takip Detay</h2>
<br />
<table class="table table-striped">
    @*striped table da bir koyu bir açık geliyor*@
    <tr>
        <th>Açıklama</th>
        <th>Tarih-Zaman</th>

    </tr>
    @foreach (var x in Model)

    {
        <tr>
            <td>@x.Aciklama</td>
            <td>@x.TarihZaman</td>

        </tr>
    }
</table>

